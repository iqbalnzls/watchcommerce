# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Product {
  id: Int!
  name: String!
  brandID: Int!
  price: Int!
  quantity: Int!
}

type Brand {
  name: String!
}

type Query {
  getProduct(id: Int!): Product!
  getProductsByBrandId(brandID: Int!): [Product!]!
}

type Mutation {
  createBrand(name: String!): Brand
}